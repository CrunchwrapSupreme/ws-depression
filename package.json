{
  "name": "@crunchwrapsupreme/ws-depression",
  "version": "0.0.2",
  "description": "ws-discovery over UDP multicast.",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "/dist",
    "/badges"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test-ci": "jest --ci --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageReporters": [
      "clover",
      "json",
      "lcov",
      "text",
      "json-summary"
    ],
    "verbose": false,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/0ld",
      "/__tests__/data",
      "/__tests__/lib"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CrunchwrapSupreme/ws-depression.git"
  },
  "keywords": [
    "soap",
    "ws-discovery",
    "satan"
  ],
  "author": "David Young",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CrunchwrapSupreme/ws-depression/issues"
  },
  "homepage": "https://github.com/CrunchwrapSupreme/ws-depression#readme",
  "devDependencies": {
    "@types/node": "^20.4.1",
    "@types/uuid": "^9.0.2",
    "jest": "^29.3.1",
    "semistandard": "^17.0.0",
    "typedoc": "^0.24.8"
  },
  "dependencies": {
    "follow-redirects": "^1.15.2",
    "string-escape-map": "^1.0.0",
    "uuid": "^9.0.0"
  },
  "types": "./dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
