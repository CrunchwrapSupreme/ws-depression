{
  "name": "ws-depression",
  "version": "1.0.0",
  "description": "ws-discovery over UDP multicast.",
  "main": "index.js",
  "files": [
    "/lib",
    "/badges"
  ],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageReporters": [
      "clover",
      "json",
      "lcov",
      "text",
      "json-summary"
    ],
    "verbose": false,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/0ld",
      "/__tests__/data",
      "/__tests__/lib"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CrunchwrapSupreme/ws-depression.git"
  },
  "keywords": [
    "soap",
    "ws-discovery",
    "satan"
  ],
  "author": "David Young",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CrunchwrapSupreme/ws-depression/issues"
  },
  "homepage": "https://github.com/CrunchwrapSupreme/ws-depression#readme",
  "devDependencies": {
    "jest": "^29.3.1",
    "standard": "^17.1.0"
  },
  "dependencies": {
    "follow-redirects": "^1.15.2",
    "string-escape-map": "^1.0.0",
    "uuid": "^9.0.0"
  }
}
